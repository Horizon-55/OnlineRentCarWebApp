@{
    ViewData["Title"] = "Вкладка поверення";
    var GetData = (IEnumerable<Returns>)ViewData["ReturnedCars"];
}
<body>
    <nav class="navbar navbar-expand-lg navbar-light bg-primary">
        <div class="container-fluid">
            <a class="navbar-brand" asp-controller="ControllerMainAdminPanel" asp-action="MainAdminPanel">Car Rent 1.0</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
                <div class="navbar-nav">
                    <a class="nav-link active" aria-current="page" asp-controller="ControllerMainAdminPanel" asp-action="MainAdminPanel">Домашня сторінка</a>
                    <a class="nav-link" asp-controller="ControlerCars" asp-action="Cars">Автомобілі</a>
                    <a class="nav-link" asp-controller="ControllerCustoners" asp-action="Customers">Клієнти</a>
                    <a class="nav-link" asp-controller="ControllerRents" asp-action="Rents">Оренда</a>
                    <a class="nav-link" asp-controller="ControllerReturns" asp-action="Returns">Повернення</a>
                    <a class="nav-link" asp-controller="ControllerLogin" asp-action="LoginPage">Вийти</a>
                </div>
            </div>
        </div>
    </nav>
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-5"></div>
            <div class="col-md-3"><img src="/img/car.png" height="180px">
                <h1 class="text-warning">Повернуті Автомобілі</h1>    
            </div>
            <div class="col-md-3"></div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <table class="table table-bordered table-striped" style="width:100%">
                    <thead>
                        <tr>
                            <th>Id поверення автомобіля</th>
                            <th>Номер авто</th>
                            <th>Id Користувача</th>
                            <th>Дата повернення</th>
                            <th>Затримки після оренди в к-сть днях</th>
                            <th>Комісії</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (GetData != null)
                        {
                            @foreach (var obj in GetData)
                            {
                                <tr>
                                    <td width="30%">
                                        @obj.RentId
                                    </td>
                                    <td width="30%">
                                        @obj.Car
                                    </td>
                                    <td width="30%">
                                        @obj.User
                                    </td>
                                    <td width="30%">
                                        @obj.Date
                                    </td>
                                    <td width="30%">
                                        @obj.Delay
                                    </td>
                                    <td width="30%">
                                        @obj.Fine
                                    </td>
                                </tr>
                            }
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</body>